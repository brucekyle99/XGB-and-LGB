    "\n",
    "    'max_depth': 6,\n",
    "\n",
    "    'min_data': 5,\n",
    "\n",
    "    'min_hessian': 1,\n",
    "\n",
    "    'verbose': -1\n",
    "\n",
    "}\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "train_preds_lgb = np.zeros(train_feat.shape[0])\n",
    "\n",
